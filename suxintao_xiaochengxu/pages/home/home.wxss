.angle{
  background: #fff;
  width: 300rpx;
  height: 100rpx;
  text-align: center;
  box-shadow: 0px 15rpx 10rpx -5rpx #C5BDBC;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.angle-wrapper{
  width: 300rpx;
  height: 100rpx;
  text-align: center;
  position: relative;  
  box-sizing: border-box;
  background: #e88885;
  display: flex;
  flex-direction: column;
  justify-content: center;
  box-shadow: 0px 15rpx 10rpx -5rpx #C5BDBC;
}
.angle-wrapper:before { 
    content: '';  
    width: 0;  
    height: 0;  
    border: 10px solid transparent;  
    border-top-color: #e88885;  
    position: absolute;  
    left: 50%;  
    margin-left: -10px;
    top: 100%;
    box-sizing: border-box;
}
